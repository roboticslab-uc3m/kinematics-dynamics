<!-- leftLeg.kinbody.xml  -->

<KinBody name="leftLeg">
    <Body name="leftLeg0" type="dynamic">
        <Geom type="trimesh">
        </Geom>
        <mass type="sphere">
            <total>.1</total>
            <radius>1</radius>
        </mass>
    </Body>
    <Body name="leftLeg1">
        <Geom type="trimesh">
            <data>./leftLeg1.wrl 0.001 </data>
            <render>./leftLeg1.wrl 0.001 </render>
        </Geom>
        <mass type="sphere">
            <total>.1</total>
            <radius>1</radius>
        </mass>
    </Body>
    <Joint name="q7" type="hinge">
        <offsetfrom>leftLeg0</offsetfrom>
        <body>leftLeg0</body>
        <body>leftLeg1</body>
        <axis>0 0 -1</axis>
        <anchor>0 0.146 0</anchor>
        <limitsdeg>-90 90</limitsdeg>
        <maxveldeg>7.5</maxveldeg>
    </Joint>

    <Body name="leftLeg2">
        <Geom type="trimesh">
            <data>./leftLeg2.wrl 0.001 </data>
            <render>./leftLeg2.wrl 0.001 </render>
        </Geom>
        <mass type="sphere">
            <total>.1</total>
            <radius>1</radius>
        </mass>
    </Body>
    <Joint name="q8" type="hinge">
        <offsetfrom>leftLeg0</offsetfrom>
        <body>leftLeg1</body>
        <body>leftLeg2</body>
        <axis>-1 0 0</axis>
        <anchor>0 0.146 -0.092</anchor>
        <limitsdeg>-20 20</limitsdeg>
        <maxveldeg>7.5</maxveldeg>
    </Joint>

    <Body name="leftLeg3">
        <Geom type="trimesh">
            <data>./leftLeg3.wrl 0.001 </data>
            <render>./leftLeg3.wrl 0.001 </render>
        </Geom>
        <mass type="sphere">
            <total>.1</total>
            <radius>1</radius>
        </mass>
    </Body>
    <Joint name="q9" type="hinge">
        <offsetfrom>leftLeg0</offsetfrom>
        <body>leftLeg2</body>
        <body>leftLeg3</body>
        <axis>0 1 0</axis>
        <anchor>0 0 -0.092</anchor>
        <limitsdeg>-30 45</limitsdeg>
        <maxveldeg>7.5</maxveldeg>
    </Joint>

</KinBody>


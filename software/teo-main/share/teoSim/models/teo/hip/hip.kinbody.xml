<!-- hip.kinbody.xml  -->

<!-- no joint limits -->
<KinBody name="hip">
    <!--Body type="dynamic" name="hip0"-->
    <Body type="static" name="hip0">
        <Geom type="trimesh">
            <data>./hip0.wrl 0.001 </data>
            <render>./hip0.wrl 0.001 </render>
        </Geom>
        <mass type="sphere">
            <total>.1</total>
            <radius>1</radius>
        </mass>
    </Body>
    <Body name="hip1">
        <Geom type="trimesh">
            <data>./hip1.wrl 0.001 </data>
            <render>./hip1.wrl 0.001 </render>
        </Geom>
        <mass type="sphere">
            <total>.1</total>
            <radius>1</radius>
        </mass>
    </Body>
    <Joint name="canId13" type="hinge">
        <offsetfrom>hip0</offsetfrom>
        <body>hip0</body>
        <body>hip1</body>
        <axis>0 1 0</axis>
        <maxvel>2</maxvel>
    </Joint>

    <Body name="hip2">
        <Geom type="trimesh">
            <data>./hip2.wrl 0.001 </data>
            <render>./hip2.wrl 0.001 </render>
        </Geom>
        <mass type="sphere">
            <total>.1</total>
            <radius>1</radius>
        </mass>
    </Body>
    <Joint name="canId14" type="hinge">
        <offsetfrom>hip0</offsetfrom>
        <body>hip1</body>
        <body>hip2</body>
        <axis>0 0 1</axis>
        <anchor></anchor>
        <maxvel>2</maxvel>
    </Joint>

</KinBody>

